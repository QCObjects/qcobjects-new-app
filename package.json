{
  "name": "qcobjectsnewapp",
  "version": "1.2.87",
  "description": "App template for New App in QCObjects",
  "main": "js/init.js",
  "scripts": {
    "test": "(npx eslint *.js js/*.js js/packages/*.js --fix) && (npx jasmine)",
    "sync": "git add . && git commit -am ",
    "preversion": "npm i --upgrade && npm test",
    "postversion": "git push && git push --tags && npm publish",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "start": "node app.js",
    "build": "exit 0"
  },
  "repository": {
    "type": "file",
    "url": "https://github.com/QuickCorp/qcobjects-new-app.git"
  },
  "keywords": [
    "qcobjects",
    "newapp",
    "js",
    "javascript"
  ],
  "author": "Jean Machuca (correojean@gmail.com)",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-qcobjects": "^0.0.13",
    "jasmine": "^3.6.3",
    "qcobjects": "^2.3.29",
    "qcobjects-cli": "latest"
  },
  "dependencies": {
    "grunt": "^1.4.1",
    "grunt-contrib-jasmine": "^2.0.2",
    "nyc": "^15.1.0",
    "qcobjects-sdk": "latest"
  }
}
